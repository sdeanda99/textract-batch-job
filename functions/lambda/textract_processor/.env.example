# AWS Textract Processor Configuration
# Copy this file to .env and update with your values
# DO NOT commit .env to git (it's in .gitignore)

# ============================================================================
# AWS Configuration
# ============================================================================
AWS_PROFILE=default
AWS_REGION=us-east-1

# ============================================================================
# S3 Input Configuration (where PDFs are stored)
# ============================================================================
SOURCE_BUCKET=your-source-bucket-name-here
BATCH_PREFIX_PATTERN=batch-  # Pattern for batch folders (e.g., batch-1/, batch-2/)

# ============================================================================
# S3 Output Configuration (where Textract results are saved)
# ============================================================================
# Can be the same as SOURCE_BUCKET or a different bucket
OUTPUT_BUCKET=your-output-bucket-name-here
OUTPUT_PREFIX=processed/  # Prefix for processed results (e.g., processed/, results/, or empty)

# ============================================================================
# Local Results Configuration
# ============================================================================
OUTPUT_RESULTS_DIR=../textract_results  # Local directory for downloaded results

# ============================================================================
# AWS Resource Naming (customize if needed to avoid conflicts)
# ============================================================================
DYNAMODB_TABLE_NAME=textract-jobs
SNS_TOPIC_NAME=textract-completion-topic
SQS_QUEUE_NAME=textract-results-queue

# ============================================================================
# IAM Role Names
# ============================================================================
TEXTRACT_ROLE_NAME=textract-sns-role
LAMBDA_INITIATOR_ROLE_NAME=lambda-textract-initiator-role
LAMBDA_PROCESSOR_ROLE_NAME=lambda-textract-processor-role

# ============================================================================
# Lambda Function Names
# ============================================================================
LAMBDA_INITIATOR_NAME=textract-batch-initiator
LAMBDA_PROCESSOR_NAME=textract-result-processor

# ============================================================================
# Lambda Configuration
# ============================================================================
LAMBDA_INITIATOR_TIMEOUT=300
LAMBDA_INITIATOR_MEMORY=512
LAMBDA_PROCESSOR_TIMEOUT=300
LAMBDA_PROCESSOR_MEMORY=1024
LAMBDA_RUNTIME=python3.12

# ============================================================================
# Processing Configuration
# ============================================================================
TOTAL_BATCHES=29  # Total number of batch folders to process
